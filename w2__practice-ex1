#pragma warning(disable: 4996)
#include <stdio.h>

int main()
{
	char ch = 'c';
	char *chptr = &ch;

	int i = 20;
	int *intptr = &i;

	const char *ptr = "I am a string";

	printf("\n [%c], [%d], [%c], [%d] ,[%d], [%c], [%s]\n", *chptr, *intptr, *ptr, ptr, ptr + 5, *(ptr + 5), ptr);
	getchar();

	return 0;
}
